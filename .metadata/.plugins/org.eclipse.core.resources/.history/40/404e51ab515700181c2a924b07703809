package com.beans;

public class Commande
{
	private String nomClient;
	private String prenomClient;
	private String adresseClient;
	private String telClient;
	private String emailClient;
	private String modeDePaiement;
	private String statutPaiement;
	private String modeDeLivraison;
	private String statutLivraison;
	private String date;
	private double montant;

	/************************
	 * GETTER/SETTER *
	 ************************/

	public String getNomClient()
	{
		return nomClient;
	}

	public String getPrenomClient()
	{
		return prenomClient;
	}

	public String getAdresseClient()
	{
		return adresseClient;
	}

	public String getTelClient()
	{
		return telClient;
	}

	public String getEmailClient()
	{
		return emailClient;
	}

	public String getModeDePaiement()
	{
		return modeDePaiement;
	}

	public String getStatutPaiement()
	{
		return statutPaiement;
	}

	public String getModeDeLivraison()
	{
		return modeDeLivraison;
	}

	public String getStatutLivraison()
	{
		return statutLivraison;
	}

	public double getMontant()
	{
		return montant;
	}

	public void setNomClient(String nom)
	{
		nomClient = nom;
	}

	public void setPrenomClient(String prenom)
	{
		prenomClient = prenom;
	}

	public void setAdresseClient(String adresse)
	{
		adresseClient = adresse;
	}

	public void setTelClient(String tel)
	{
		telClient = tel;
	}

	public void setEmailClient(String email)
	{
		emailClient = email;
	}

	public void setModeDePaiement(String modeDePaiement)
	{
		this.modeDePaiement = modeDePaiement;
	}

	public void setStatutPaiement(String statutPaiement)
	{
		this.statutPaiement = statutPaiement;
	}

	public void setModeDeLivraison(String modeDeLivraison)
	{
		this.modeDeLivraison = modeDeLivraison;
	}

	public void setStatutLivraison(String statutLivraison)
	{
		this.statutLivraison = statutLivraison;
	}

	public void setMontant(double montant)
	{
		this.montant = montant;
	}

	public String getDate()
	{
		return date;
	}

	public void setDate(String date)
	{
		this.date = date;
	}
	
	
}
